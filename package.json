{
  "name": "review-flow-action",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub action for automating PR review flow",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"**/*.@(ts|js|json)\"",
    "format-check": "prettier --check \"**/*.@(ts|js|json)\"",
    "lint": "eslint src --ext ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "eventology",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.2",
    "@actions/github": "^2.1.1"
  },
  "devDependencies": {
    "@eventology/eslint-config": "^2.0.1",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@zeit/ncc": "^0.21.1",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "jest-circus": "^25.1.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "~3.8.2"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "arrowParens": "always",
    "quoteProps": "consistent"
  },
  "eslintConfig": {
    "extends": "@eventology/eslint-config"
  }
}
