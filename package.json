{
  "name": "review-flow-action",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub action for automating PR review flow",
  "main": "build/index.js",
  "scripts": {
    "build": "webpack",
    "format": "prettier --write \"**/*.@(ts|js|json)\"",
    "format-check": "prettier --check \"**/*.@(ts|js|json)\"",
    "lint": "eslint src --ext ts",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "eventology",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.2",
    "@actions/github": "^2.1.1",
    "ts-node": "^8.6.2"
  },
  "devDependencies": {
    "@eventology/eslint-config": "^2.0.1",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/webpack": "^4.41.6",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "jest-circus": "^25.1.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "~3.8.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "arrowParens": "always",
    "quoteProps": "consistent"
  },
  "eslintConfig": {
    "extends": "@eventology/eslint-config"
  }
}
