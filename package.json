{
  "name": "review-flow-action",
  "version": "1.1.1",
  "private": true,
  "description": "GitHub action for automating PR review flow",
  "main": "build/index.js",
  "scripts": {
    "build": "webpack",
    "format": "prettier --write \"**/*.@(ts|js|json)\"",
    "format-check": "prettier --check \"**/*.@(ts|js|json)\"",
    "lint": "eslint src --ext ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "ci": "npm-run-all --parallel --print-label build lint test typecheck",
    "postinstall": "typesync",
    "postuninstall": "typesync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "eventology",
  "license": "MIT",
  "devDependencies": {
    "@actions/core": "^1.2.3",
    "@actions/github": "^2.1.1",
    "@eventology/eslint-config": "^2.0.1",
    "@octokit/rest": "^16.0.0",
    "@types/eslint": "^6.1.8",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@types/prettier": "^1.19.0",
    "@types/webpack": "^4.41.7",
    "eslint": "^6.8.0",
    "inquirer": "^7.0.6",
    "jest": "^25.1.0",
    "jest-circus": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "~3.8.3",
    "typesync": "^0.6.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "arrowParens": "always",
    "quoteProps": "consistent"
  },
  "eslintConfig": {
    "extends": "@eventology/eslint-config",
    "rules": {
      "import/order": "off"
    }
  }
}